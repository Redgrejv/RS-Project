{
        "name": "server",
        "version": "0.0.6",
        "scripts": {
                "gulp": "node node_modules/gulp/bin/gulp.js",
                "front": "npm run gulp build && npm run gulp webserver | npm run gulp watch",
                "front_dev": "npm run gulp build_dev && npm run gulp webserver | npm run gulp watch_dev",
                "app": "node server.js",
                "server": "mkdir -p ~/data/db/ | mkdir C:/data/db | mongod --dbpath ~/data/db | mongod --dbpath C:/data/db | npm run app",
                "dev": "npm run server | npm run front_dev",
                "prod": "npm ren server | npm run front"
        },
